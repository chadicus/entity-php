{
    "name": "chadicus/entity",
    "description": "Base classes for basic API entities",
    "require": {
        "php": "~5.6",
        "dominionenterprises/filter": "~1.0",
        "dominionenterprises/util": "~1.0",
        "chadicus/spl-exceptions": "~1.0",
        "chadicus/spl-traits": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "chadicus/coding-standard": "~1.1",
        "satooshi/php-coveralls": "~0.6.1"
    },
    "autoload": {
        "psr-4": {
            "Chadicus\\Entity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChadicusTest\\Entity\\": "tests"
        }
    }
}
